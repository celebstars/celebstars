    <%-include("../navbar/navbar.ejs")%>
    <div class="profile">
      <div class="profile-header">
        <a href="/profileUpdate"
          ><button type="button" class="btn btn-outline-primary">
            프로필 관리하기
          </button></a
        >
      </div>
    </div>
    <!-- profile -->
    <main class="profile">
      <div class="profile-container">
        <div class="profile-inner">
          <img
            class="profile-img"
            src="/static/img/profile/user1/main4.jpg"
            alt=""
          />
          <div class="profile-content">
            <div class="container text-center">
              <div class="row">
                <div class="col-12 profile-name">한나나</div>
              </div>
              <div class="row profile-description">
                <div class="col-2 offset-2">
                  <p><strong>생년월일</strong></p>
                  <p>xx.xx.xx</p>
                </div>
                <div class="col-2">
                  <p><strong>Height</strong></p>
                  <p>xxxcm</p>
                </div>
                <div class="col-2">
                  <p><strong>Weight</strong></p>
                  <p>xxkg</p>
                </div>
                <div class="col-2">
                  <p><strong>3 Size</strong></p>
                  <p>xx-xx-xx</p>
                </div>
              </div>
              <!-- cards -->
              <div class="row row-cols-1 row-cols-md-3">
                <div class="col mt-3">
                  <div class="card h-100">
                    <div class="card-body">
                      <h5 class="card-title">드라마</h5>
                      <p class="card-text">2022 08</p>
                    </div>
                  </div>
                </div>
                <div class="col mt-3">
                  <div class="card h-100">
                    <div class="card-body">
                      <h5 class="card-title">영화</h5>
                      <p class="card-text">2022 08</p>
                    </div>
                  </div>
                </div>
                <div class="col mt-3">
                  <div class="card h-100">
                    <div class="card-body">
                      <h5 class="card-title">광고</h5>
                      <p class="card-text">2022 08</p>
                    </div>
                  </div>
                </div>
                <div class="col mt-3">
                  <div class="card h-100">
                    <div class="card-body">
                      <h5 class="card-title">잡지</h5>
                      <p class="card-text">2022 08</p>
                    </div>
                  </div>
                </div>
                <div class="col mt-3">
                  <div class="card h-100">
                    <div class="card-body">
                      <h5 class="card-title">무대</h5>
                      <p class="card-text">2022 08</p>
                    </div>
                  </div>
                </div>
                <div class="col mt-3">
                  <div class="card h-100">
                    <div class="card-body">
                      <h5 class="card-title">TV</h5>
                      <p class="card-text">2022 08</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- Button trigger modal -->
            <div class="ful-screen">
              <a href="/magazine">
                <button type="button" class="btn btn-outline-primary">
                  <strong> 프로필 보기 </strong>
                </button>
              </a>
              <!-- <button
                type="button"
