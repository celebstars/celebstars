        <%-include("./text_scroll/text_scroll.ejs")%>
        <div id="banner_box" class="carousel slide" data-bs-ride="true">
          <div class="carousel-indicators">
            <button
              type="button"
              data-bs-target="#banner_box"
              data-bs-slide-to="0"
              class="active"
              aria-current="true"
              aria-label="Slide 1"
            ></button>
            <% for(var i=2; i<4; i++) { %>
            <button
              type="button"
              data-bs-target="#banner_box"
              data-bs-slide-to="<%=i-1%>"
              aria-label="Slide <%=i%>"
            ></button>
            <% } %>
          </div>
          <div class="banner_img" class="carousel-inner">
            <div class="carousel-item active">
              <a href="/"
                ><img
                  src="/static/img/homepage/main/main_banner1.jpg"
                  class="d-block w-100"
              /></a>
            </div>
            <% for(var i=2; i<4; i++) { %>
            <div class="carousel-item">
              <a href="/"
                ><img
                  src="/static/img/homepage/main/main_banner<%=i%>.jpg"
                  class="d-block w-100"
              /></a>
            </div>
            <% } %>
          </div>
          <button
            class="carousel-control-prev"
            type="button"
            data-bs-target="#banner_box"
            data-bs-slide="prev"
          >
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button
            class="carousel-control-next"
            type="button"
            data-bs-target="#banner_box"
            data-bs-slide="next"
          >
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
        </div>
