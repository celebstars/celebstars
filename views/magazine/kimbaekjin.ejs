<body class="magazine_body">
  <div class="magazine_container">
    <!-- 화면의 버튼들 -->
    <section class="close_button">
      <button id='closeBtn' class='close_button_value'>X</button>
    </section>

    <section class="button_down">
      <img src="/static/img/artistImg/page_down_arrow_action.gif" />
    </section> 
    <!-- 프로필 사진 래퍼 -->
    <div class="magazine_bg_img">
      <div class="main_img">
        <div class="artist_name">김 백 진</div>
      </div>
    </div>

    <div class="magazine_bg_img">
      <div class="img_filter">
        <div class="posting_img_wrapper magazine_posting_img">
          <div class='magazine_posting_img2'>
            <div class="artist_info_wrapper right">
              <div class="artist_info">
                <p class="info_text">생년월일</p>
                <p class="info_text">키</p>
                <p class="info_text">몸무게</p>
                <p class="info_text">3-사이즈</p>
                <p class="info_text">신발 사이즈</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="magazine_bg_img">
      <div class="img_filter">
        <div class="posting_img_wrapper magazine_posting_img">
          <div class='magazine_posting_img3'></div>
        </div>
      </div>
    </div>

    <div class="magazine_bg_img">
      <div class="img_filter">
        <div class="posting_img_wrapper magazine_posting_img">
          <div class='magazine_posting_img4'>
            <div class="artist_info_wrapper right">
              <div class="artist_career">
                <% for (let i=0; i<2; i++) { %>
                <section class="career_wrapper">
                  <p class="career_title">경력 타이틀</p>
                  <% for (let j=0; j<9; j++) {%>
                  <p class="career_content">경력 내용</p>
                  <% } %>
                </section>
                <% } %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="magazine_bg_img">
      <div class="img_filter">
        <div class="posting_img_wrapper magazine_posting_img">
          <div class='magazine_posting_img5'>
